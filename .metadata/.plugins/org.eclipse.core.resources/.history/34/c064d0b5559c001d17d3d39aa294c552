package introtojava;

public class ASCIIExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		

		    char c = 'A';
		    int ascii = c;
		    System.out.println("The ASCII value of " + c + " is: " + ascii);

		    // output: The ASCII value of A is: 65
		}

	}
